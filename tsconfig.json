{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@libs/*": ["src/libs/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@handlers/*": ["src/handlers/*"],
    }
  }
}
